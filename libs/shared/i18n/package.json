{
  "name": "@org/shared-i18n",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@org/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "name": "i18n",
    "tags": [
      "scope:shared",
      "type:i18n"
    ],
    "sourceRoot": "libs/shared/i18n/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/shared/i18n/dist",
          "main": "libs/shared/i18n/src/index.ts",
          "tsConfig": "libs/shared/i18n/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  }
}
